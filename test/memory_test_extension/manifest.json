{
	"manifest_version": 2,

	"name": "ConstraintJS Test Extension",
	"description": "This extension helps test Interstate.",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},

	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_start"
		}
	],

	"permissions": [
		"tabs",
		"*://*/*",
		"debugger"
	]
}
